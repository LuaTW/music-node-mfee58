<%- include("parts/html-head") %> <%- include("parts/html-navbar") %>
<div class="container">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th scope="col-6">員工編號</th>
        <th scope="col-6">姓名</th>
        <th scope="col-6">年齡</th>
      </tr>
    </thead>
    <tbody>
      <%for (let s of sales){ %>
      <tr>
        <td><%= s.id %></td>
        <td><%= s.name %></td>
        <td><%= s.age %></td>
      </tr>
      <%}%>
    </tbody>
  </table>
</div>
